+                                           source={require('../../images/me_bj.jpg')}
+                                    >
+
+                                    </Image>
+
+
+
+
+                                </View>
+                            )
+                            return (
+                                <TouchableOpacity style={{ width: w, height: h }} key={i} onPress={() => { this.onItemClick(item) }}>{render}</TouchableOpacity>
+                            )
+                        })
+                    }
